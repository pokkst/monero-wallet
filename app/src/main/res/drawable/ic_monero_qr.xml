<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:padding="10dp"
            android:shape="oval">
            <padding
                android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"
                android:top="2dp" />
            <stroke
                android:width="1dp"
                android:color="#fff" />
            <solid android:color="#fff" />
        </shape>
    </item>
    <item android:drawable="@drawable/ic_monero" />
</layer-list>
