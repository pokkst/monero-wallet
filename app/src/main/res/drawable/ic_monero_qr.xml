<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="oval" android:padding="10dp">
            <padding
                android:top="2dp"
                android:bottom="2dp"
                android:left="2dp"
                android:right="2dp"/>
            <stroke
                android:color="#fff"
                android:width="1dp"/>
            <solid
                android:color="#fff"/>
        </shape>
    </item>
    <item android:drawable="@drawable/ic_monero" />
</layer-list>
